/* 娃娃奖品  */

.recriveview {
  margin-left: 25rpx;
  margin-right: 25rpx;
  height: 120rpx;
  background-color: rgb(249, 245, 205);
}

.recriveview text {
  margin-left: 30rpx;
  font-size: 14px;
  text-align: center;
}

.goreceive {
  position: absolute;
  right: 52rpx;
  width: 200rpx;
  height: 52rpx;
  padding-top: 9rpx;
  font-size: 14px;
  text-align: center;
  border-radius: 42rpx;
  background-color: rgba(250, 128, 102, 1);
  color: #5d6979;
}

.gift {
  margin-top: 20rpx;
  height: 500rpx;
  margin-left: 25rpx;
  margin-right: 25rpx;
  background-color: #efeb8b;
  box-shadow: 2rpx 2rpx 1rpx #aaa;
}

.giftimage {
  height: 420rpx;
  width: 100%;
}

.gift image {
  height: 420rpx;
  width: 100%;
}

.giftname {
  height: 73rpx;
  padding-top: 10rpx;
  text-align: center;
  background-color: #fff;
}

/* 弹出层 layer  */

.layer-content {
    position: relative;
}

.layer-content icon {
    position: absolute;
    top: 0;
    right: 0;
    /* width: 40rpx;
    height: 40rpx; */
    transform: translate(50%, -50%);
    background-color: #fff;
    border-radius: 50%;
}

.layer-content .layer-title {
    text-align: center;
    margin-bottom: 30rpx;
    color: #5d6979;
    /* background-color: #5d6979; */
}

/* 规则 */

.layer-content .rule-text {
    color: #666;
    margin-bottom: 20rpx;
}

.rule-text .rule-index {
    width: 44rpx;
    height: 44rpx;
    text-align: center;
    line-height: 44rpx;
    border: solid 1px rgb(143, 123, 242);
    color: rgb(143, 123, 242);
    margin-right: 20rpx;
    border-radius: 50%;
}

.layer-content .service {
    display: block;
    width: 340rpx;
    height: 70rpx;
    color: #5d6979;
    font-size: 15px;
    /* border: solid 1px #5d6979; */
    text-align: center;
    line-height: 70rpx;
    border-radius: 8rpx;
    margin: 0 auto;
}

.layer-content .gocontact {
  margin: 30rpx 25% 0rpx 25%;
  width: 50%;
  line-height: 66rpx;
  text-align: center;
  font-size: 14px;
  border-radius: 33rpx;
  color: #fff;
  background-color: rgb(143, 123, 242);
}


