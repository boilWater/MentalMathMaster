}

.listheadinfo {
  width: 75rpx;
  height: 75rpx;
  margin-left: 30rpx;
  border-radius: 50%; 
  background-color: rgb(180, 160, 246);
}

.listheadinfo image {
  width: 75rpx;
  height: 75rpx;
  /* margin-top: 15rpx;
  margin-left: 35rpx; */
  border-radius: 50%;
  background-color: rgb(209, 207, 207);
}

.listname {
  margin-left: 15rpx;
  font-size: 14px;
  color: #5d6979;
}

.listnumber {
  position: absolute;
  width: 220rpx;
  right: 40rpx;
  text-align: center;
  font-size: 12px;
  color: #aaa;
