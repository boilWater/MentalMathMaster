.total {
  text-align: center;
  font-size: small;
  color: #fff;
  margin-bottom: 25rpx;
}

.startchallenge {
  /* position: absolute; */
  margin-left: 100rpx;
  text-align: center;
  width: 540rpx;
  line-height: 78rpx;
  border-radius: 45rpx;
  color: rgb(121, 51, 212);
  background-color: #6d41e2;
  margin-bottom: 25rpx;
  border-width: 8rpx;
  border-color:  rgb(121, 51, 212);
  border-style: solid;
  background: linear-gradient(to bottom, #eff125, #eeba28);  
  /* transform: translate(50%, 50%); */
}

.invitefriends { 
  margin-left: 100rpx;
  text-align: center;
  width: 560rpx;
  line-height: 78rpx;
  /* padding: 13rpx; */
  margin-right: 150rpx;
  margin-bottom: 30rpx;
  border-radius: 38rpx;
  background-color: rgba(353, 252, 252, 0.3);
  color: rgb(254, 254, 255);
}


/* yellowbottom 内容部分 */

.yellowbottom {
  position: absolute; 
  top: 700rpx;  
  width: 100%;
  /* height: 900rpx; */
  background-color: #eade46;
  margin-bottom: 60rpx;
}

.ranklist {
  margin: 25rpx 25rpx 15rpx 25rpx;
  /* background-color: #ccc; */
}

.honour {
  width: 32%;
  text-align: center;
  line-height: 66rpx;
  font-size: 14px;
  /* padding-top: 6rpx; */
  border-color: #7b61ef;
  border-left-width: 1rpx;
  border-top-width: 1rpx;
  border-bottom-width: 1rpx;
  border-right-width: 0rpx;
  border-right-style: 0rpx;
  border-style: solid;
  color: rgb(209, 207, 207);
}

.perseverance {
  width: 34%;
  line-height: 66rpx;
  font-size: 14px;
  /* padding-top: 6rpx; */
  text-align: center;
  border-color: #7b61ef;
  border-width: 1rpx; 
  border-style: solid;
  color: rgb(209, 207, 207);
}

.prize {
  width: 32%;
  line-height: 66rpx;
  font-size: 14px;
  /* padding-top: 6rpx; */
  text-align: center;
  border-color: #7b61ef;
  border-right-width: 1rpx;
  border-top-width: 1rpx;
  border-bottom-width: 1rpx;
  border-left-width: 0rpx;
  border-style: solid;
  color: rgb(209, 207, 207);
}

.selectorcolor {
  background-color: rgb(143, 123, 242);
  color: rgb(254, 254, 255);
}

.unselectorcolor {
  background-color: #fff;
  color: #7b61ef;
}

.yellowscroll {
  height: 40rpx;
  background-color: rgb(254, 154, 12);
}

