/* 信息展示界面 & 挑战 & 分享 */

page {
  background-color: #eade46;
}

.bluetop {
  position: absolute; 
  top: 0rpx;
  width: 100%;
  height: 760rpx;
  background-color: rgb(143, 123, 242);
  /* border-bottom-left-radius: 50%;
  border-bottom-right-radius: 50%; */
}

.challenge {
  margin-top: 30rpx;
  width: 100%;
  height: 180rpx;
  /* margin-top: 10rpx; */
  text-align: center; 
   /* background-color: #fae45e;  */
}

.challenge image {
  width: 540rpx;
  height: 108rpx;
}

.challengetext {
  position: absolute;
  width: 100%; 
  /* width: 66%; */
  top: 75rpx;
  /* text-align: right; */
   text-align: center;     
}

.challengetext .wintext {
  width: 64%;
  font-size: smaller;
  color: rgba(353, 252, 252, 1);
  text-align: right;
  /* background-color: #ccc;   */
}

.challengetext image {
  margin-left: 8rpx;
  width: 46rpx;
  height: 46rpx;
}

.slogan {
  text-align: center;
  font-size: x-large;
  color: rgba(253, 252, 252, 1);
  font-weight: bold;
  margin-bottom: 30rpx;
}

